alignment top_right
background yes
border_width 1
cpu_avg_samples 2
default_color 993300
color0 993300
default_outline_color 993300
default_shade_color 993300
double_buffer yes
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades no
gap_x 15
gap_y 15
maximum_width 210
max_port_monitor_connections 64
max_specials 512
max_user_text 16384
minimum_size 330 10
net_avg_samples 2
no_buffers yes
out_to_console no
wm_class_name Conky
own_window_colour black
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_transparent yes
own_window_type normal
own_window yes
stippled_borders 2
update_interval 2
uppercase no
use_spacer yes
use_xft yes
xftalpha 0.8
xftfont  Bitstream Vera Sans Mono:size=9
 
TEXT
${color0}$sysname $kernel ${alignr}$color${time %k:%M:%S}
${machine} @ $freq(MHz)
 
${color0}CPU Frequency:${color}${alignr}${freq}MHz
${color0}CPU Temperature:${color}${alignr}${texeci 30 sensors | grep "Package id 0" | cut -d ':' -f 2 | cut -d '(' -f 1 | tr -d ' '}
${color0}CPU Usage:${color}${alignr}${cpu}% 
${color0}${cpugraph cpu0 32,210 993300 993300} 
#${color0}Core4:$color ${cpu cpu4}%$alignr${exec sensors | grep 'Core 3' | cut -c16-22 | sed '/^$/d'}
#${color0}${cpugraph cpu2 32,210 993300 993300} 
${color0}MEMORY: $color$memperc%$alignr$mem${color0}/${color}$memmax
${color0}${memgraph 32,210  993300 993300}
SWAP usage: $swapperc% 
Used: $swap/$swapmax

NETWORK:
${color0}Down: $color${downspeed enp5s0}$alignr${totaldown enp5s0}
${color0}${downspeedgraph enp5s0 32,210 993300 993300}
${color0}Up:   $color${upspeed enp5s0}$alignr${totalup enp5s0}
${color0}${upspeedgraph enp5s0 32,210 993300 993300}
DRIVES:
${color0}/$alignr$color${fs_used /}${color0}/$color${fs_size /}
${color0}${fs_bar 5,210 /}
${color0}/home$alignr$color${fs_used /home}${color0}/$color${fs_size /home}
${color0}${fs_bar 5,210 /home}
${color0}//mnt/media$alignr$color${fs_used /mnt/media}${color0}/$color${fs_size /mnt/media}
${color0}${fs_bar 5,210 /mnt/media}
${color0}/mnt/backup$alignr$color${fs_used /mnt/backup}${color0}/$color${fs_size /mnt/backup}
${color0}${fs_bar 5,210 /mnt/backup}
${color0}/mnt/data$alignr$color${fs_used /mnt/data}${color0}/$color${fs_size /mnt/data}
${color0}${fs_bar 5,210 /mnt/data}
${color0}/mnt/virt$alignr$color${fs_used /mnt/virt}${color0}/$color${fs_size /mnt/virt}
${color0}${fs_bar 5,210 /mnt/virt}
${color0}/mnt/ext4$alignr$color${fs_used /mnt/ext4}${color0}/$color${fs_size /mnt/ext4}
${color0}${fs_bar 5,210 /mnt/ext4}

${color0}Read: $color${diskio_read}$alignr${color0}Write: $color${diskio_write}
${color0}${diskiograph /dev/sda 32,210 993300 993300}
